#list_pic{
	width:100%;
	height:300px;
	background:url(../images/about2.png) no-repeat;
}

strong,p,a{
	letter-spacing: 0.1em;
}
h5{
	font-weight: 600;
	letter-spacing: 0.1em;
}
ul li{
	list-style-type: none;
}
header{
	height: 220px;
	background-image: url(../images/N1.png); 
	width: 100%;
}
#content{
	padding-top: 100px;
}
#search{
	margin-right: 10%;
}

.more ul{
	padding: 0;
	margin: 0;
}
.more ul li{
	width: 98%;
}
.more ul li a{
	text-decoration: none;
	color: #222;
	font-size: 10px;
	line-height: 2em;
	overflow: hidden;
    	white-space: nowrap;
    	text-overflow: "";
}
.more ul li a:hover{
	color: #1DA1F2;
}
.more  ul li .right_more{
	font-size: 10px;
	line-height: 2em;
	float: right;
}