
.mask{
    width:100%;
	height:100%;
	background:rgba(0,0,0,0.4);
	position:fixed;
	top:0;
	left:0;
	z-index:100;
	display:none;
}
.begin-voide{
    width:70%;
	position:fixed;
	top:8%;
	left:15%;
	z-index:110;
	background:#fff;
	border-radius:5px;
	display:none;
	
}
.cancel{
    width:60px;
	height:60px;
	position:fixed;
	top:8%;
	right:2%;
	z-index:110;
	background:url(../images/cancel.png);
	display:none;
}

#top_pic{
	width:100%;
	height:300px;
	background:url(../images/about2.png) no-repeat;
}
#about_js{
	width:100%;
	height:500px;
}
#js_text{
	border-left:#E0E0E0 1px solid;
	border-right:#E0E0E0 1px solid;
	margin-top:50px;
	width:100px;
	height:50px;
}
#pic_js{
	width:100%;
	height:90%;
	position:relative;
}

#about_js_text{
	width:100%;
	height:auto;
	position:absolute;
	margin-top:20px;
}
#about_video{
	width:100%;
	height:450px;
	margin-top:30px;
	background:url(../images/N1.png);		
	text-align:center;
	color:#fff;
	 background-size:cover;
}

#about_team{
	width:100%;
	height:400px;
    margin: 20px auto;
    overflow: hidden;
    position: relative;
}
#about_map{
	width:100%;
	height:350px;
	margin-top:20px;
	
}













