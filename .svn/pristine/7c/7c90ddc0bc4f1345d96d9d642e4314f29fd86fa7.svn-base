<script>
$(function(){
	$("#top").click(function(){
		  $("html,body").animate({scrollTop:0}, 500);
		});
		
	  $("#begin").click(function(){
			    $(".mask").show();
				$(".begin-voide").show();
			    $(".cancel").show();
				 document.getElementById("play").play();
			  });
			  
			  $(".cancel").click(function(){
			     $(".mask").hide();
				$(".begin-voide").hide();
			    $(".cancel").hide();
				 document.getElementById("play").pause();
			  
			  });	
	
})
</script>	
