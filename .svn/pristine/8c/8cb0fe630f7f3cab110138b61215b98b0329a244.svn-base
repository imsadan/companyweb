}
.mask{
    width:100%;
	height:100%;
	background:rgba(0,0,0,0.4);
	position:fixed;
	top:0;
	left:0;
	z-index:100;
	display:none;
}
.begin-voide{
    width:70%;
	position:fixed;
	top:8%;
	left:15%;
	z-index:110;
	background:#fff;
	border-radius:5px;
	display:none;
	
}
.cancel{
    width:60px;
	height:60px;
	position:fixed;
	top:8%;
	right:2%;
	z-index:110;
	background:url(../images/cancel.png);
	display:none;
}

