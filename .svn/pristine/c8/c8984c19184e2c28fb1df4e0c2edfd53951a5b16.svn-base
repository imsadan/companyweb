#read_pic{
	width:100%;
	height:300px;
	background:url(../images/about2.png) no-repeat;
}

strong,p,a{
	letter-spacing: 0.1em;
}
h5{
	font-weight: 600;
	letter-spacing: 0.1em;
}
ul li{
	list-style-type: none;
}
header{
	height: 220px;
	background-image: url(../images/N1.png); 
	width: 100%;
}
#content{
	padding-top: 100px;
}
#search{
	margin-right: 10%;
}
.massage{
	padding-bottom: 10%;
}
.massage embed{
	width: 98%;
	height: 260px;
}
.title_massage{
	margin-top: 20px;
	margin-bottom: 20px;
}
.title_massage span{
	color: #666;
	font-size: 8;
}

.body_massage p{
	font-family: Microsoft YaHei;
	height: 2.4em;
	line-height: 1.2em;
	letter-spacing:0.1em;
	overflow: hidden;
    	white-space: pre-wrap;
    	text-overflow: clip;
}
.read_massage{
	margin-top: 40px;
	margin-bottom: 40px;
}
.read_massage a{
	width: 20%;
	letter-spacing: 0.1em;
	font-size: 12px;
	color: #222;
	border:1px solid #222;
	border-radius: 100px;
	border-top-left-radius: 100px;
	border-top-right-radius: 100px;
	border-bottom-right-radius: 100px;
	border-bottom-left-radius: 100px;
}
.more ul{
	padding: 0;
	margin: 0;
}
.more ul li{
	width: 98%;
}
.more ul li a{
	text-decoration: none;
	color: #222;
	font-size: 10px;
	line-height: 2em;
	overflow: hidden;
    	white-space: nowrap;
    	text-overflow: "";
}
.more ul li a:hover{
	color: #1DA1F2;
}
.more  ul li .right_more{
	font-size: 10px;
	line-height: 2em;
	float: right;
}